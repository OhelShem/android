    <!-- Intro -->
    <string name="intro_dashboard_primary_text">שלום!</string>
    <string name="intro_dashboard_secondary_text">ברוכים הבאים לאפליקציה שתקל על חייכם בתור תלמידים ומורים בתיכון אהל שם. להמשך לחצו כאן.</string>

    <string name="intro_bottombar_timetable_primary_text">מערכת שעות</string>
    <string name="intro_bottombar_timetable_secondary_text">פה נמצאת המערכת שלכם. רוצים למחוק שעה שאתם לא לומדים בה? לתת לשיעור שם מצחיק? לבחור את המקבץ? לחצו על כפתור העריכה כדי לערוך את המערכת.</string>
    <string name="intro_bottombar_timetable_secondary_text_teacher">במסך זה תוכלו לראות את מערכת השעות שלכם ושל כל הכיתות בבית הספר. לחיצה שנייה על הכפתור תפתח את המערכת השבועית. בשיעורים עם מקבץ ניתן ללחוץ על השעה כדי להציג את הכיתות לשיעור</string>

    <string name="intro_bottombar_changes_primary_text">שינויים</string>
    <string name="intro_bottombar_changes_secondary_text">כאן תוכלו לראות את השינויים של הכיתה והשכבה שלכם. האפליקציה תתריע לכם בזמן אמת על כל שינוי במערכת לפני ובמהלך היום.</string>
    <string name="intro_bottombar_changes_secondary_text_teacher">שינויים יומיים במערכת השעות עבור כל שכבה</string>

    <string name="intro_bottombar_dates_primary_text">מועדים</string>
    <string name="intro_bottombar_dates_secondary_text">במסך זה תוכלו למצוא רשימה של כל המבחנים והחופשים הקרובים שלכם. יש לשים לב לעדכונים של לוח המבחנים.</string>
    <string name="intro_bottombar_dates_secondary_text_teacher">מבחנים וחגים בבית הספר</string>

    <string name="intro_bottombar_contacts_primary_text">הכיתה שלי</string>
    <string name="intro_bottombar_contacts_secondary_text">איזור זה מיועד למציאת פרטי ההתקשרות של חבריכם לכיתה יחד עם ימי הולדת בכיתה ובבית הספר.</string>
    <string name="intro_bottombar_contacts_secondary_text_teacher">מספרי טלפון, ימי הולדת בבית הספר ועוד</string>

    <string name="intro_menu_primary_text">רגע, זה לא הכל!</string>
    <string name="intro_menu_secondary_text">ערכות נושא, התראות, הגדרות ומידע נוסף אפשר למצוא פה. את פרטיכם תוכלו לעדכן באתר בית הספר. תהנו!</string>

    <string name="intro_overrides_primary_text">חשוב לגבות</string>
    <string name="intro_overrides_secondary_text">מי יודע מה צופן העתיד? רעידת אדמה עלולה להשמיד עיר שלמה באפריקה. אז כדאי שתגבה את העריכות שעשית ליתר ביטחון.</string>

    <!-- Yoav Sternberg -->
